<div class="rentlook_form">
  <% form_for @property, :html=>{ :multipart=>true} do |f| %>
    <%= f.error_messages %>
    <fieldset>
      <legend>Edit property details</legend>
      <div>
        <%= f.label :landlord_id %>
        <%= f.text_field :landlord_id %>
      </div>
      <div>
        <%= f.label :name %>
        <%= f.text_field :name %>
      </div>
      <div>
        <%= f.label :address %>
        <%= f.text_field :address %>
      </div>
      <div>
        <%= f.label :city %>
        <%= f.text_field :city %>
      </div>
      <div>
        <%= f.label :state %>
        <%= f.text_field :state %>
      </div>
      <div>
        <%= f.label :zip %>
        <%= f.text_field :zip %>
      </div>
      <div>
        <%= f.label :country %>
        <%= f.text_field :country %>
      </div>
      <div>
        <%= f.label :photo %>
        <%=f.file_field :photo%>
      </div>
      <div>
        <%= f.submit 'Update' %>
      </div>
    </fieldset>
  <% end %>
</div>
<%= link_to 'Show', @property %> |
<%= link_to 'Back', properties_path %>